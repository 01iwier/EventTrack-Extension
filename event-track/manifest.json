{
  "manifest_version": 3,
  "name": "Event Track",
  "version": "1.0",
  "description": "View UFC and other sports events.",
  "permissions": ["storage"],
  "host_permissions": [
      "http://localhost:3000/*"  
  ],
  "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
  }
}